    "karma": "^6.0.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.0",
    "karma-jasmine": "^5.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^17.3.0",
    "protractor": "^7.0.0",
    "ts-node": "^10.0.0",
    "tsickle": "^0.39.1",
    "tslib": "^2.4.0",
    "tslint": "^6.1.0",
    "typescript": "^5.4.5"
  }
